{
  "schemaVersion": 1,
  "id": "kaimyentity",
  "version": "${version}",

  "name": "KAIMyEntity",
  "description": "A forge & fabric mod to replace any entity with MMD models in Minecraft ",
  "authors": [
    "kjkjkAIStudio",
    "asuka-mio"
  ],
  "contact": {
    "sources": "https://github.com/asuka-mio/KAIMyEntity"
  },

  "license": "MIT",
  "icon": "assets/kaimyentity/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.kAIS.KAIMyEntity.KAIMyEntity"
    ],
    "client": [
      "com.kAIS.KAIMyEntity.KAIMyEntityClient"
    ]
  },
  "mixins": [
    "kaimyentity.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.0",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
